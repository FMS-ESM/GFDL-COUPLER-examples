  <experiment name="c96_atmos_domain_decomp">
<!--
c96 means 96x96 grid points per face
fv_layout=X,Y means a domain decomposition of iec=96/X , jec=96/Y per face.
E.g.  fv_layout="3,24" gives:
  X-AXIS =   32  32  32
  Y-AXIS =    4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4   4

NOTE: In the follwoing npes is the number of PE cores reserved but it may NOT be equal to the actual number of PE's used.
      Since many of the following configurations add to the same number of npes, I arbitarily incremented npes by 1 to distingush them.
      This would cost one node (32 cores) of resources.
      We should go back to use the override mechanism of FRE to avoid this.  
-->    
    <input>
      <csh><![CDATA[
# Some possible domain decompositionsn.
set do_ocean = "false"
set do_concurrent_radiation = ".false."
set domains_stack_size = "2097152"

if ( "$npes" == "432" ) then
  set atmos_npes = "432"
  set atmos_nthreads = "1"
  set nxblocks = "4" ; set nyblocks = "2" 
  set fv_layout    =   "3,24";   set fv_io_layout    =  "1,4"
  set land_layout  =   "3,24";   set land_io_layout  =  "1,1"
  set ocean_layout =   "144,3";  set ocean_io_layout =  "1,3"
  set ice_layout   =   "144,3";  set ice_io_layout   =  "1,1"

else if ( "$npes" == "864" ) then
  set atmos_npes = "864"
  set atmos_nthreads = "1"
  set nxblocks = "2" ; set nyblocks = "2" 
  set fv_layout    =   "6,24";   set fv_io_layout    =  "1,4"
  set land_layout  =   "6,24";   set land_io_layout  =  "1,1"
  set ocean_layout =   "144,6";  set ocean_io_layout =  "1,3"
  set ice_layout   =   "144,6";  set ice_io_layout   =  "1,1"

else if ( "$npes" == "865" ) then
  set atmos_npes = "432"
  set atmos_nthreads = "2"
  set nxblocks = "4" ; set nyblocks = "2" 
  set fv_layout    =   "3,24";   set fv_io_layout    =  "1,4"
  set land_layout  =   "3,24";   set land_io_layout  =  "1,1"
  set ocean_layout =   "144,3";  set ocean_io_layout =  "1,3"
  set ice_layout   =   "144,3";  set ice_io_layout   =  "1,1"

#Don't know how to run with  atmos_nthreads = "3". I get:
#aprun -d 3 -n 288 ./fms_cm4p3_v20141222.x
#apsched: claim exceeds reservation's node-count
#
#else if ( "$npes" == "866" ) then
#  set atmos_npes = "288"
#  set atmos_nthreads = "3"
#  set nxblocks = "3" ; set nyblocks = "1" 
#  set fv_layout    =   "3,16";   set fv_io_layout    =  "1,4"
#  set land_layout  =   "3,16";   set land_io_layout  =  "1,1"
#  set ocean_layout =   "144,2";  set ocean_io_layout =  "1,2"
#  set ice_layout   =   "144,2";  set ice_io_layout   =  "1,1"

else if ( "$npes" == "867" ) then
  set atmos_npes = "216"
  set atmos_nthreads = "4"
  set nxblocks = "4" ; set nyblocks = "4" 
  set fv_layout    =   "3,12";  set fv_io_layout    =  "1,4"
  set land_layout  =   "3,12";  set land_io_layout  =  "1,1"
  set ocean_layout =   "72,3";  set ocean_io_layout =  "1,3"
  set ice_layout   =   "72,3";  set ice_io_layout   =  "1,1"

#aprun -d 6 -n 144 ./fms_cm4p3_v20141222.x
#apsched: claim exceeds reservation's node-count
#else if ( "$npes" == "869" ) then
#  set atmos_npes = "144"
